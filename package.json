{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "description": "Velammal College QA Examination Platform",
  "author": "Velammal Engineering College",
  "publish": {
    "provider": "github",
    "owner": "Ajith-ajay",
    "repo": "my-electron-demo"
  },
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently -k -r \"npm:dev\" \"wait-on tcp:5173 && electron .\"",
    "build:web": "vite build",
    "build:electron": "vite build && electron-builder --win --x64 --publish=never",
    "preview": "vite preview",
    "lint": "eslint .",
    "release": "dotenv -e .env.production -- sh -c \"vite build && electron-builder --win --x64 --publish=always\"",
    "release:check": "dotenv -e .env.production -- node -e \"console.log(!!process.env.GH_TOKEN)\""
  },
  "dependencies": {
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "framer-motion": "^12.25.0",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "styled-components": "^6.3.5",
    "sweetalert2": "^11.26.17",
    "universal-cookie": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "form-data": "^4.0.0",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "wait-on": "^9.0.3",
    "workbox-window": "^7.4.0"
  },
  "build": {
    "appId": "com.qa.examination",
    "productName": "QA Examination Platform",
    "copyright": "Copyright Â© 2025 Velammal Engineering College",
    "publish": {
      "provider": "github",
      "owner": "Ajith-ajay",
      "repo": "my-electron-demo"
    },
    "generateUpdatesFilesForAllChannels": true,
    "icon": "public/VEC_Logo.ico",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json",
      ".env.production"
    ],
    "asarUnpack": [
      "dist/**/*",
      ".env.production"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/VEC_Logo.ico",
      "artifactName": "${productName} Setup ${version}.${ext}"
    },
    "mac": {
      "target": "dmg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Velammal QA Exam",
      "installerIcon": "public/VEC_Logo.ico",
      "uninstallerIcon": "public/VEC_Logo.ico",
      "installerHeader": "public/VEC_Logo.ico",
      "displayLanguageSelector": false
    },
    "directories": {
      "output": "release"
    }
  }
}
